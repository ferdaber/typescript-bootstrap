{
  "parser": "typescript-eslint-parser",
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["typescript", "prettier"],
  "env": {
    "jest": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off"
  }
}

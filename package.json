{
  "name": "typescript-bootstrap",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --open --config config/webpack.config.js",
    "build": "webpack --config config/webpack.config.js",
    "build:dev": "NODE_ENV=development npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "test": "NODE_ENV=test jest --config config/jest.config.js",
    "test:watch": "npm test -- --watchAll",
    "ts": "tsc",
    "ts:watch": "tsc --watch",
    "lint": "eslint --config config/.eslintrc.js 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "npm lint --fix"
  },
  "dependencies": {
    "core-js": "^3.6.5",

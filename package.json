    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.4.0",
    "linaria": "^1.3.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "sass-loader": "^8.0.2",
